{
  "compilerOptions": {
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "strict": true,
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "declaration": true,
    "lib": [
      "es2016"
    ],
    "sourceMap": true
  },
  "exclude": [
    "node_modules",
    "**/__tests__/*",
    "server/setupJest.ts",
    "testing"
  ]
}
